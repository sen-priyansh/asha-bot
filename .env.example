# Example environment file for Asha Discord Bot
# Copy to .env and fill in real values. Do NOT commit your .env.

# Discord Bot Token (required) — from https://discord.com/developers/applications
DISCORD_TOKEN=

# Google Gemini API Key (optional unless using AI chat) — from Google AI Studio
GEMINI_API_KEY=

# Bot owner Discord user ID (numeric)
OWNER_ID=

# Default auto-role ID (optional)
DEFAULT_ROLE_ID=

# Activity and presence (optional)
BOT_ACTIVITY=Moderating & Chatting
BOT_STATUS=online

# Gemini generation settings (optional)
GEMINI_MODEL=gemini-2.0-flash
MAX_TOKENS=1024
TEMPERATURE=0.7
TOP_P=0.95
TOP_K=40

# AI chat behavior toggles
REPLY_TO_PINGS=true
REPLY_TO_REPLIES=true